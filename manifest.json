{
  "manifest_version": 3,
  "name": "Interactive Quiz Builder",
  "version": "1.0",
  "description": "Create interactive quizzes with your own questions and a timer.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
